# API Configuration
# Base URL for the Soky Recargas API
VITE_API_BASE_URL=https://stg-api.sokyrecargas.com/api/v1

VITE_APP_TOKEN=soky-auth-token-example

# Application Configuration
# Timeout for API requests in milliseconds
VITE_API_TIMEOUT=30000

# Security Configuration
# Secret key for AES encryption of sensitive data in sessionStorage
# IMPORTANT: Change this value in production for better security
VITE_ENCRYPTION_SECRET=secret

DOCKER_PORT=80
DOCKER_CONTAINER_NAME=soky-client-ui